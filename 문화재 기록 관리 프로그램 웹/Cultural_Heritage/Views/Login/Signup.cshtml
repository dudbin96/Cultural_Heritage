@model Cultural_Heritage.Models.Login
@{
    ViewBag.Title = "Signup";
}
<h2>회원가입</h2>
@using (Html.BeginForm("Signuppost", "Login", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <table>
        @*<tr><td>구 분</td><td>@Html.TextBoxFor(m => m.num)</td></tr>*@
        <tr><td>아이디</td><td>@Html.TextBoxFor(m => m.id)</td></tr>
        <tr><td>패스워드</td><td>@Html.TextBoxFor(m => m.password)</td></tr>



        <tr><td colspan="2"><input type="submit" value="가입" class="btn btn-default" /></td></tr>
    </table>
}